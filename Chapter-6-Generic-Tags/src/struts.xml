<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />

	<package name="stuff" extends="struts-default">
		<action name="Bean" class="stuff.BeanTestAction">
			<result>/jsp/Bean.jsp</result>
		</action>

		<action name="Date" class="stuff.DateTestAction">
			<result>/jsp/Date.jsp</result>
		</action>

		<action name="Generator">
			<result>/jsp/Generator.jsp</result>
		</action>

		<action name="GeneratorConverter" class="stuff.GeneratorConverterTestAction">
			<result>/jsp/GeneratorConverter.jsp</result>
		</action>

		<action name="If" class="stuff.IfTestAction">
			<result>/jsp/If.jsp</result>
		</action>

		<action name="Iterator" class="stuff.IteratorTestAction">
		    <result>/jsp/Iterator.jsp</result>
		</action>

		<action name="Merge" class="stuff.MergeTestAction">
		    <result>/jsp/Merge.jsp</result>
		</action>

		<action name="Property" class="stuff.PropertyTestAction">
		    <result>/jsp/Property.jsp</result>
		</action>

		<action name="Push" class="stuff.PushTestAction">
		    <result>/jsp/Push.jsp</result>
		</action>

		<action name="Set" class="stuff.SetTestAction">
		    <result>/jsp/Set.jsp</result>
		</action>

		<action name="Sort" class="stuff.SortTestAction">
		    <result>/jsp/Sort.jsp</result>
		</action>

		<action name="Subset" class="stuff.SubsetTestAction">
		    <result>/jsp/Subset.jsp</result>
		</action>
    </package>
</struts>
